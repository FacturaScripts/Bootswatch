{% extends "@Core/Master/MenuTemplate.html.twig" %}

{% block css %}
    {% set theme = appSettings.get('bootswatch', 'theme') %}
    {% if theme == 'bootstrap' or theme == '' or theme == null %}
        {% set cssBs = 'node_modules/bootstrap/dist/css/bootstrap.min.css' %}
    {% else %}
        {% set cssBs = 'Plugins/Bootswatch/node_modules/bootswatch/dist/' ~ theme ~ '/bootstrap.min.css' %}
    {% endif %}
    <link rel="stylesheet" href="{{ asset(cssBs) }}" />
    <link rel="stylesheet" href="{{ asset('node_modules/font-awesome/css/font-awesome.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('node_modules/jquery-ui-dist/jquery-ui.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('Dinamic/Assets/CSS/custom.css') }}" />
{% endblock %}

{% block navbar %}
    {% set navbarTheme = appSettings.get('bootswatch', 'navbar-theme') %}
    <nav class="navbar navbar-expand-lg {{ navbarTheme }} sticky-top d-print-none">
        {% block navbarContent %}
            {{ parent() }}
        {% endblock %}
    </nav>
{% endblock %}